{
	"env": {
		"es2021": true,
		"node": true
	},
	"extends": [
		"xo",
		"plugin:react/recommended",
		"prettier",
		"prettier/react"
	],
	"overrides": [
		{
			"extends": [
				"xo-typescript"
			],
			"files": [
				"*.ts",
				"*.tsx"
			]
		}
	],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": [
		"react",
		"react-native"
	],
	"rules": {}
}